LDLIBS  = -lm
CFLAGS  = -O3 -Ofast -Wall -march=native -msse4 -msha
LDFLAGS = -fopenmp -msse4 -msha
objects = benchmark.o sha256-x86.o dict.o util/memory.o



all: benchmark

benchmark: $(objects)



benchmark.o: sha256.h types.h util/util_char_arrays.h shared.h sha256-x86.h
sha256-x86.o: sha256-x86.h
dict.o: dict.h types.h shared.h
util/memory.o: util/memory.h



.PHONY: clean
clean:
	rm -f $(objects)
